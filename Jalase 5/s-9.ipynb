   "source": [
    "user_input = int(input(\"لطفاً یک عدد بین 1 تا 50 وارد کنید: \"))\n",
    "found = False  \n",
    "\n",
    "for num in range(1, 51):  \n",
    "    if num == user_input: \n",
    "        found = True   \n",
    "        print(\"عدد پیدا شد!\")\n",
    "        break  \n",
    "\n",
    "if not found: \n",
    "    print(\"عدد پیدا نشد.\")\n"
   ]
  }
