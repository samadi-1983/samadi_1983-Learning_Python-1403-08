    "def multiply_ten_numbers():\n",
    "    product = 1\n",
    "    \n",
    "    for i in range(10):\n",
    "      \n",
    "        number = int(input(f\"لطفاً عدد صحیح {i + 1} را وارد کنید: \"))\n",
    "        product *= number  \n",
    "    \n",
    "    return product\n",
    "\n",
    "\n",
    "result = multiply_ten_numbers()\n",
    "print(f\"محصول 10 عدد وارد شده: {result}\")\n"
   ]
  }
